#!/bin/bash

dir_name="TestDir"
mkdir -p "$dir_name"

cd "$dir_name"

for i in {1..10}; do
        file_name="File${i}.txt"
        echo "$file_name" > "$file_name"
done

echo "Created $dir_name with files File1.txt to File10.txt."
~                                                                                    
~   
